{
  "name": "overdraft",
  "version": "0.4.0",
  "description": "Enhanced Rich-Text Editor using Draft.js",
  "main": "lib/Overdraft.js",
  "scripts": {
    "start": "webpack-dev-server --content-base demo/ --inline",
    "build:demo": "BUILD_DEMO=1 webpack",
    "build": "babel src --copy-files -d lib",
    "lint": "eslint src demo",
    "test": "echo no tests yet"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/meriadec/overdraft.git"
  },
  "keywords": [
    "draft.js"
  ],
  "author": "Meriadec Pillet",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/meriadec/overdraft/issues"
  },
  "homepage": "https://github.com/meriadec/overdraft#readme",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": [
      "zavatta",
      "zavatta-react"
    ],
    "rules": {
      "no-undefined": 0
    },
    "globals": {
      "__VERSION__": false
    }
  },
  "dependencies": {
    "draft-convert": "^1.4.8",
    "draft-js": "^0.10.1",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "eslint": "^4.4.1",
    "eslint-config-zavatta": "^6.0.1",
    "eslint-config-zavatta-react": "^2.2.2",
    "eslint-plugin-react": "^7.2.0",
    "react": "^15.6.1",
    "react-color": "^2.13.4",
    "react-dom": "^15.6.1",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10"
  }
}
